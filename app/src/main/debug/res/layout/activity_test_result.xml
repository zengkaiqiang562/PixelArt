<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey_f3">

    <View
        android:id="@+id/stub_bar"
        android:layout_width="0dp"
        android:layout_height="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@id/test_result_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/stub_bar"
        android:layout_marginTop="2dp"
        android:layout_marginStart="20dp"
        android:src="@drawable/icon_back"/>

    <TextView
        android:id="@id/test_result_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/stub_bar"
        android:layout_marginTop="16dp"
        android:text="@string/test_result"
        android:textColor="@color/black_33"
        android:textSize="21sp"
        android:textStyle="bold"
        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="350dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/test_result_back"
        android:layout_marginTop="5dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="11dp"
        android:background="@drawable/test_exec_chart_bg"
        >

        <ImageView
            android:id="@id/test_result_server_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="44dp"
            android:layout_marginStart="35dp"
            android:src="@drawable/test_exec_server_icon"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@id/test_result_server_icon"
            app:layout_constraintEnd_toEndOf="@id/test_result_server_icon"
            app:layout_constraintTop_toBottomOf="@id/test_result_server_icon"
            android:layout_marginTop="6dp"
            android:text="@string/internet"
            android:textColor="@color/black_33"
            android:textSize="12sp"
            />

        <ImageView
            android:id="@id/test_result_client_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="44dp"
            android:layout_marginEnd="47dp"
            android:src="@drawable/test_exec_client_icon"
            />

        <TextView
            android:id="@id/test_result_client_name"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@id/test_result_client_icon"
            app:layout_constraintEnd_toEndOf="@id/test_result_client_icon"
            app:layout_constraintTop_toBottomOf="@id/test_result_client_icon"
            android:layout_marginTop="6dp"
            tools:text="Redmi K30 5G Redmi K30 5G"
            android:maxLines="2"
            android:ellipsize="end"
            android:gravity="center"
            android:textColor="@color/black_33"
            android:textSize="12sp"
            />

        <LinearLayout
            android:id="@id/test_result_trans_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/test_result_server_icon"
            app:layout_constraintBottom_toBottomOf="@id/test_result_server_icon"
            android:orientation="horizontal"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/test_point_icon"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/test_point_icon"
                android:layout_marginStart="25dp"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/test_point_icon"
                android:layout_marginStart="25dp"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/test_point_icon"
                android:layout_marginStart="25dp"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/test_point_icon"
                android:layout_marginStart="25dp"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/test_point_icon"
                android:layout_marginStart="25dp"
                />

        </LinearLayout>

        <ImageView
            android:id="@id/test_result_tx_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/test_result_server_icon"
            app:layout_constraintBottom_toTopOf="@id/test_result_trans_container"
            android:layout_marginBottom="5dp"
            android:layout_marginStart="25dp"
            android:src="@drawable/test_tx_icon"
            />

        <TextView
            android:id="@id/test_result_tx_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/test_result_tx_icon"
            app:layout_constraintTop_toTopOf="@id/test_result_tx_icon"
            app:layout_constraintBottom_toBottomOf="@id/test_result_tx_icon"
            android:layout_marginStart="3dp"
            tools:text="10Mbps"
            android:textColor="@color/black_33"
            android:textSize="12sp"
            />

        <ImageView
            android:id="@id/test_result_rx_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toStartOf="@id/test_result_rx_value"
            app:layout_constraintTop_toTopOf="@id/test_result_tx_icon"
            android:layout_marginEnd="3dp"
            android:src="@drawable/test_rx_icon"
            />

        <TextView
            android:id="@id/test_result_rx_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toStartOf="@id/test_result_client_icon"
            app:layout_constraintTop_toTopOf="@id/test_result_rx_icon"
            app:layout_constraintBottom_toBottomOf="@id/test_result_rx_icon"
            android:layout_marginEnd="25dp"
            tools:text="21Mbps"
            android:textColor="@color/black_33"
            android:textSize="12sp"
            />

        <TextView
            android:id="@id/test_result_delay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@id/test_result_trans_container"
            app:layout_constraintEnd_toEndOf="@id/test_result_trans_container"
            app:layout_constraintTop_toBottomOf="@id/test_result_trans_container"
            android:layout_marginTop="8dp"
            android:textColor="@color/black_33"
            android:textSize="12sp"
            tools:text="199ms"
            />

        <TextView
            android:id="@id/test_result_ssid_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/test_result_client_name"
            android:layout_marginStart="29dp"
            android:layout_marginTop="30dp"
            android:text="@string/ssid"
            android:textSize="16sp"
            android:textColor="@color/black_33"
            />

        <TextView
            android:id="@id/test_result_ssid_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@id/test_result_ssid_name"
            android:layout_marginEnd="29dp"
            tools:text="Sinozo-A-5G"
            android:textSize="16sp"
            android:textColor="@color/alpha_99_33"
            />

        <TextView
            android:id="@id/test_result_mode_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@id/test_result_ssid_name"
            app:layout_constraintTop_toBottomOf="@id/test_result_ssid_name"
            android:layout_marginTop="15dp"
            android:text="@string/phy_mode"
            android:textSize="16sp"
            android:textColor="@color/black_33"
            />

        <TextView
            android:id="@id/test_result_mode_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/test_result_ssid_value"
            app:layout_constraintBaseline_toBaselineOf="@id/test_result_mode_name"
            tools:text="WiFi(802.11ac)"
            android:textSize="16sp"
            android:textColor="@color/alpha_99_33"
            />

        <TextView
            android:id="@id/test_result_speed_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@id/test_result_ssid_name"
            app:layout_constraintTop_toBottomOf="@id/test_result_mode_name"
            android:layout_marginTop="15dp"
            android:text="@string/phy_speed"
            android:textSize="16sp"
            android:textColor="@color/black_33"
            />

        <TextView
            android:id="@id/test_result_speed_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/test_result_ssid_value"
            app:layout_constraintBaseline_toBaselineOf="@id/test_result_speed_name"
            tools:text="↓ 20Mbps ↑ 10Mbps"
            android:textSize="16sp"
            android:textColor="@color/alpha_99_33"
            />

        <TextView
            android:id="@id/test_result_signal_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@id/test_result_ssid_name"
            app:layout_constraintTop_toBottomOf="@id/test_result_speed_name"
            android:layout_marginTop="15dp"
            android:text="@string/signal"
            android:textSize="16sp"
            android:textColor="@color/black_33"
            />

        <TextView
            android:id="@id/test_result_signal_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/test_result_ssid_value"
            app:layout_constraintBaseline_toBaselineOf="@id/test_result_signal_name"
            tools:text="-69dBm"
            android:textSize="16sp"
            android:textColor="@color/alpha_99_33"
            />

        <TextView
            android:id="@id/test_result_dns_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@id/test_result_ssid_name"
            app:layout_constraintTop_toBottomOf="@id/test_result_signal_name"
            android:layout_marginTop="15dp"
            android:text="@string/dns_server"
            android:textSize="16sp"
            android:textColor="@color/black_33"
            />

        <TextView
            android:id="@id/test_result_dns_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/test_result_ssid_value"
            app:layout_constraintBaseline_toBaselineOf="@id/test_result_dns_name"
            tools:text="198.19.255.254"
            android:textSize="16sp"
            android:textColor="@color/alpha_99_33"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>