<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey_f3">

    <View
        android:id="@+id/stub_bar"
        android:layout_width="0dp"
        android:layout_height="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@id/more_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/stub_bar"
        android:layout_marginTop="10dp"
        android:textSize="21sp"
        android:textColor="@color/black_33"
        android:textStyle="bold"
        android:text="@string/more"
        />

    <ImageView
        android:id="@id/more_button_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/more_title"
        app:layout_constraintBottom_toBottomOf="@id/more_title"
        android:layout_marginEnd="12dp"
        android:src="@drawable/test_icon"
        />

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/more_title"
        app:layout_constraintBottom_toTopOf="@id/home_menu_root"
        android:layout_marginBottom="6dp"
        android:layout_marginTop="8dp"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@id/more_privacy_root"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:orientation="horizontal"
                android:background="@drawable/wifi_item_bg"
                android:gravity="center_vertical"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="29dp"
                    android:src="@drawable/more_icon1"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/more_privacy_policy"
                    android:textSize="16sp"
                    android:textColor="@color/black_33"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@id/more_service_root"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:orientation="horizontal"
                android:background="@drawable/wifi_item_bg"
                android:gravity="center_vertical"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="29dp"
                    android:src="@drawable/more_icon2"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/more_terms_of_service"
                    android:textSize="16sp"
                    android:textColor="@color/black_33"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@id/more_rate_root"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:orientation="horizontal"
                android:background="@drawable/wifi_item_bg"
                android:gravity="center_vertical"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="29dp"
                    android:src="@drawable/more_icon3"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/more_rate_us"
                    android:textSize="16sp"
                    android:textColor="@color/black_33"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@id/more_feedback_root"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:orientation="horizontal"
                android:background="@drawable/wifi_item_bg"
                android:gravity="center_vertical"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="29dp"
                    android:src="@drawable/more_icon4"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/more_feedback"
                    android:textSize="16sp"
                    android:textColor="@color/black_33"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@id/more_share_root"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:orientation="horizontal"
                android:background="@drawable/wifi_item_bg"
                android:gravity="center_vertical"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="29dp"
                    android:src="@drawable/more_icon5"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/more_share"
                    android:textSize="16sp"
                    android:textColor="@color/black_33"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@id/more_about_root"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:orientation="horizontal"
                android:background="@drawable/wifi_item_bg"
                android:gravity="center_vertical"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="29dp"
                    android:src="@drawable/more_icon6"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:text="@string/more_about_us"
                    android:textSize="16sp"
                    android:textColor="@color/black_33"
                    />

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

    <RelativeLayout
        android:id="@id/home_menu_root"
        android:layout_width="0dp"
        android:layout_height="94dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/home_menu_bg"
        >

        <LinearLayout
            android:id="@id/home_menu_wifi_parent"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toStartOf="@id/home_menu_vpn_parent"
            android:layout_marginEnd="65dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@id/home_manu_wifi_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/menu_wifi_unsel"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/wifi"
                android:textSize="11sp"
                android:textColor="@color/_4D333333"
                />

        </LinearLayout>

        <LinearLayout
            android:id="@id/home_menu_vpn_parent"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@id/home_manu_vpn_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/menu_vpn_unsel"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/vpn"
                android:textSize="11sp"
                android:textColor="@color/_4D333333"
                />

        </LinearLayout>

        <LinearLayout
            android:id="@id/home_menu_mine_parent"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@id/home_menu_vpn_parent"
            android:layout_marginStart="65dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@id/home_manu_mine_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/menu_mine_sel"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/mine"
                android:textSize="11sp"
                android:textColor="@color/FF8A49FF"
                />

        </LinearLayout>


    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>